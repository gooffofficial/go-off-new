<script>
	function joinConvo1() {
		console.log("HEEEEYYYYY")
		var xhr = new XMLHttpRequest();
		xhr.open("POST", '/api/convos/join', true);
		xhr.setRequestHeader('Content-Type', 'application/json');
		xhr.send(JSON.stringify({
			convo: <%=convos[0].id%>
		}));
	}

	function joinConvo2() {
		console.log("HEEEEYYYYY")
