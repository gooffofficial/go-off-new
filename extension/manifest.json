{
    "name": "chat test",
    "version": "1.0", 
    "description": "chat test", 
    "permissions": ["activeTab","declarativeContent","storage"], 
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "page_action": {
        "default_popup": "index.html",
        "default_icon": {
          "16": "images/get_started16.png",
          "32": "images/get_started32.png",
          "48": "images/get_started48.png",
          "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.dev.js; object-src 'self'; script-src 'self' index.js;",

    "manifest_version": 2
}